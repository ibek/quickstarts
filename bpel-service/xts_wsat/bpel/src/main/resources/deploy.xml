<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03"
	xmlns:examples="http://www.jboss.org/bpel/examples" xmlns:airport="http://www.jboss.org/bpel/examples/airport"
	xmlns:hotel="http://www.jboss.org/bpel/examples/hotel" xmlns:pp="http://www.jboss.org/bpel/examples/pp">
	<process name="examples:BusinessTravel">
		<active>true</active>
		<retired>false</retired>
		<process-events generate="all" />
		<provide partnerLink="client">
			<service name="examples:BusinessTravelService" port="BusinessTravelPort" />
		</provide>
		<invoke partnerLink="airport">
			<service name="airport:AirportService" port="AirportServicePort" />
		</invoke>
		<invoke partnerLink="hotel">
			<service name="hotel:HotelService" port="HotelServicePort" />
		</invoke>
	</process>
	<process name="pp:ParentProcess">
		<active>true</active>
		<retired>false</retired>
		<process-events generate="all" />
		<provide partnerLink="client">
			<service name="pp:ParentProcess" port="ParentProcessPort" />
		</provide>
		<invoke partnerLink="btravel">
			<service name="examples:BusinessTravelService" port="BusinessTravelPort" />
		</invoke>
	</process>
</deploy>
